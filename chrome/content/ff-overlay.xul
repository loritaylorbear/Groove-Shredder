  <script src="toolbar.js"/>
  <script src="groove-shredder.js"/>
  
  <stringbundleset id="stringbundleset">
    <stringbundle id="grooveshredder-strings" src="chrome://grooveshredder/locale/grooveshredder.properties"/>
