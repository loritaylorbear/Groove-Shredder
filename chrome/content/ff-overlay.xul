<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="chrome://grooveshredder/skin/overlay.css" type="text/css"?>
<!DOCTYPE overlay SYSTEM "chrome://grooveshredder/locale/overlay.dtd">
<overlay id="grooveshredder-overlay" xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
